{
	"manifest_version": 2,
	"version": "1.1",
	"author": "Serd2011",
	"name": "ГУАП Расписание",
	"short_name": "ГУАП Расписание",
	"description": "ГУАП Расписание делает расписание занятий более красивым и удобным.",
	"background": {
		"scripts": [
			"js/background.js"
		]
	},
	"permissions": [
		"storage",
		"tabs",
		"https://api.guap.ru/rasp/*"
	],
	"web_accessible_resources": [
		"app/index.html"
	],
	"browser_action": {},
	"icons": {
		"16": "img/icon-16.png",
		"32": "img/icon-32.png",
		"48": "img/icon-48.png",
		"64": "img/icon-64.png",
		"128": "img/icon-128.png",
		"256": "img/icon-256.png",
		"512": "img/icon-512.png",
		"1024": "img/icon-1024.png"
	}
}